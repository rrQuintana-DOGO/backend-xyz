{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/*": ["src/*"],
      "@clients/*": ["src/clients/*"],
      "@contacts/*": ["src/contacts/*"],
      "@config/*": ["src/config/*"],
      "@common/*": ["src/common/*"],
      "@trip_types/*": ["src/trip_types/*"],
      "@liscences_types/*": ["src/liscences_types/*"],
      "@places/*": ["src/places/*"],
      "@phases/*": ["src/phases/*"],
      "@journey-types/*": ["src/journey_types/*"],
      "@status/*" : ["src/status/*"],
      "@trips/*" : ["src/trips/*"],
      "@carriers/*" : ["src/carriers/*"],
      "@routes/*" : ["src/routes/*"],
      "@drivers/*" : ["src/drivers/*"],
      "@seeders/*" : ["prisma/seeders/*"],
      "@journey_types/*" : ["src/journey_types/*"],
      "@units/*" : ["src/units/*"], 
      "@events/*" : ["src/events/*"],
      "@mongo/*" : ["src/mongoose/*"],
      "@evidences/*" : ["src/evidences/*"],
      "@places_types/*": ["src/place_types/*"],
      "@geofences/*": ["src/geofences/*"],
      "@storage/*": ["src/storage/*"],
      "@trip_logs/*": ["src/trip_logs/*"],
      "@situations/*": ["src/situations/*"],
      "@dbManager/*": ["src/db_manager/*"],
    }
  }
}
